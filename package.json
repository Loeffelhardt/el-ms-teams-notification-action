{
  "name": "typescript-action",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build",
    "all": "npm run build && npm run format && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "jdcargile",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@octokit/graphql": "^5.0.3",
    "@octokit/rest": "^19.0.5",
    "axios": "^1.1.2",
    "moment-timezone": "^0.5.32",
    "octokit": "^2.0.9"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^18.8.5",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^29.1.2",
    "jest-circus": "^29.1.2",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "@vercel/ncc": "^0.34.0"
  }
}
